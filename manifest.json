{
  "name": "Feeder grouped",
  "version": "1.0",
  "description": "Group feeds by issue - for Jira",
  "manifest_version": 2,
  "icons": {
    "16": "images/rprss16.png",
    "32": "images/rprss32.png",
    "48": "images/rprss48.png",
    "128": "images/rprss128.png"
  },
  "content_scripts": [
    {
      "css": [
        "styles.css"
      ],
      "js": [
        "lib/jquery.min.js",
        "content.js"
      ],
      "matches": [
        "https://feeder.co/reader"
      ]
    }
  ],
  "externally_connectable": {
    "matches": [
      "*://*.feeder.co/*"
    ]
  },
  "web_accessible_resources": [
    "images/*.png"
  ],
  "background": {
    "scripts": [
      "lib/hot-reload.js"
    ]
  }
}
